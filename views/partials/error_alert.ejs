    <div class="container">
        <% if(error && error.length > 0){ %>
            <div class="ui negative message" role="alert">
                <i class="close icon"></i>
                <p><%= error %></p>
            </div>
        <% } %>
        <% if(success && success.length > 0){ %>
            <div class="ui positive message" role="alert">
                <i class="close icon"></i>
                <p><%= success %></p>
            </div>
        <% } %>
    </div>
    
    
